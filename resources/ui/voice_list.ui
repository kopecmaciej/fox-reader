<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="VoiceList" parent="AdwBin">
    <property name="child">
      <object class="GtkScrolledWindow">
        <child>
          <object class="GtkColumnView" id="column_view">
            <property name="show-column-separators">false</property>
            <property name="show-row-separators">true</property>
            <property name="halign">0</property>
            <property name="hexpand">true</property>
            <!-- Accent Column -->
            <child>
              <object class="GtkColumnViewColumn" id="name_column">
                <property name="title">Voice name</property>
                <property name="expand">true</property>
              </object>
            </child>
            <!-- Traits Column -->
            <child>
              <object class="GtkColumnViewColumn" id="traits_column">
                <property name="title">Traits</property>
                <property name="fixed-width">100</property>
              </object>
            </child>
            <!-- Quality Column -->
            <child>
              <object class="GtkColumnViewColumn" id="quality_column">
                <property name="title">Quality</property>
                <property name="fixed-width">120</property>
              </object>
            </child>
            <!-- Language Column -->
            <child>
              <object class="GtkColumnViewColumn" id="language_column">
                <property name="title">Language</property>
                <property name="fixed-width">150</property>
              </object>
            </child>
            <!-- Actions Column -->
            <child>
              <object class="GtkColumnViewColumn" id="actions_column">
                <property name="title">Actions</property>
                <property name="fixed-width">140</property>
              </object>
            </child>

          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
